<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 멤버 -->  
<mapper namespace="Member">

<!-- 마이페이지 : 쿼리문 실행 확인 -->
<select id="getMypage" parameterType="donzo.thefun.model.MemberDto" resultType="donzo.thefun.model.MemberDto">
	SELECT ID, PWD, NICKNAME, PHONE, EMAIL, ADDRESS, INFO, POINT, AUTH
	FROM FUN_MEMBER
	WHERE ID=#{id}
</select>
</mapper>